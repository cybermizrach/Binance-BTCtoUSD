{
"manifest_version": 2,
    "name": "Bittrex show USD",
    "description": "Add the USD value to the output of the BTC Value column on Bittrex",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://bittrex.com/Balance"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}